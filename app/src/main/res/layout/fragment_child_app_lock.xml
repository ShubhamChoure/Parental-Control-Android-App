<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="BottomNavigation.ChildeNavigation.ChildAppLock">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/appListToolbarChild"
            android:layout_width="0dp"
            android:layout_height="?actionBarSize"
            android:background="#367CFE"
            android:padding="5dp"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:menu="@menu/option_child"
            app:title="PARENT_APP_LOCK"
            app:titleTextColor="#FFFFFF" />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/chlidAppListRV"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/appListToolbarChild" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>